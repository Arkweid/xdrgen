grammar XdrStructGrammar

  rule struct_def
    "struct" space
    identifier space? 
    struct_body space? 
    ";"
    <Xdrgen::AST::StructDef>
  end

  rule struct_body
    "{" 
    space?
    (declaration space? ";" space?)+
    "}"
  end

  rule struct_type_spec
    "struct" space struct_body
  end

end